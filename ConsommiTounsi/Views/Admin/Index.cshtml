@model ConsommiTounsi.Models.ModelKPI
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- page content -->
<div class="center-block" role="main">
    <div class="row" style="display: inline-block;">
        <div class="tile_count">
            <div class="col-md-2 col-sm-4  tile_stats_count">
                <span class="count_top"><i class="fa fa-user"></i>Total Number Of Suppliers </span>
                <div class="count">@Model.totalNumberOfSuppliers</div>
                <span class="count_bottom"><i class="green">4% </i> From last Week</span>
            </div>
            <div class="col-md-2 col-sm-4  tile_stats_count">
                <span class="count_top"><i class="fa fa-clock-o"></i>Total Number Of Custumers </span>
                <div class="count">@Model.totalNumberOfCustumers</div>
                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>3% </i> From last Week</span>
            </div>
            <div class="col-md-2 col-sm-4  tile_stats_count">
                <span class="count_top"><i class="fa fa-user"></i>Total Number Of Orders </span>
                <div class="count green">@Model.totalNumberOfOrders</div>
                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>34% </i> From last Week</span>
            </div>
            <div class="col-md-2 col-sm-4  tile_stats_count">
                <span class="count_top"><i class="fa fa-user"></i>Total Number Of Complaints </span>
                <div class="count">@Model.totalNumberOfComplaints</div>
                <span class="count_bottom"><i class="red"><i class="fa fa-sort-asc"></i>12% </i> From last Week</span>
            </div>
            <div class="col-md-2 col-sm-4  tile_stats_count">
                <span class="count_top"><i class="fa fa-user"></i>Total Number Of Deliveries</span>
                <div class="count">@Model.totalNumberOfDeliveries</div>
                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>34% </i> From last Week</span>
            </div>
            <div class="col-md-2 col-sm-4  tile_stats_count">
                <span class="count_top"><i class="fa fa-user"></i>Total Number Of Events</span>
                <div class="count">@Model.totalNumberOfEvents</div>
                <span class="count_bottom"><i class="green"><i class="fa fa-sort-asc"></i>34% </i> From last Week</span>
            </div>
